
NODE_ENV=production

BASE_URL=http://localhost
PORT=3000

SECRET_KEY=SecretKeyForEncryption
SESSION_SECRET=SecretKeyForSessionSigning

# Use MONGO_URI when (connecting to MongoDB Atlas) running locally without docker
# Read the README.md for more information
MONGO_URI=mongodb+srv://<MONGO_USER>:<MONGO_PASS>@<MONGO_HOST>/?retryWrites=true&w=majority

# Set DB_SEED true if you want to seed the database
# Read the README.md for more information
DB_SEED=false
